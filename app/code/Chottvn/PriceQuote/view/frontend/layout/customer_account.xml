<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Account price quote</title>
    </head>
    <body>
        <referenceBlock name="customer_account_navigation">
            <block after="customer-account-navigation-account-link" class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-quoteprice-link">
                <arguments>
                    <argument name="path" xsi:type="string">customer/account/pricequote</argument>
                    <argument name="label" xsi:type="string" translate="true">Account price quote</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
