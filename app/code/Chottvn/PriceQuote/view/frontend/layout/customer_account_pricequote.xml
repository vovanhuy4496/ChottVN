<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="2columns-left">
	<update handle="customer_account"/>
	<body>
		<referenceBlock name="page.main.title">
			<action method="setPageTitle">
				<argument translate="true" name="title" xsi:type="string">Request Price Quote</argument>
			</action>
		</referenceBlock>
		<referenceContainer name="content">
        <block class="Chottvn\PriceQuote\Block\Request\Listrequest" name="pricequote" template="Chottvn_PriceQuote::request/list.phtml" />
    </referenceContainer>
	</body>
</page>