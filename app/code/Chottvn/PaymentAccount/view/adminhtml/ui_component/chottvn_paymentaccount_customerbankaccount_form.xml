<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">chottvn_paymentaccount_customerbankaccount_form.customerbankaccount_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Chottvn\PaymentAccount\Block\Adminhtml\CustomerBankAccount\Edit\BackButton" name="back"/>
			<button class="Chottvn\PaymentAccount\Block\Adminhtml\CustomerBankAccount\Edit\DeleteButton" name="delete"/>
			<button class="Chottvn\PaymentAccount\Block\Adminhtml\CustomerBankAccount\Edit\SaveButton" name="save"/>
			<button class="Chottvn\PaymentAccount\Block\Adminhtml\CustomerBankAccount\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>chottvn_paymentaccount_customerbankaccount_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>chottvn_paymentaccount_customerbankaccount_form.customerbankaccount_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="customerbankaccount_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Chottvn\PaymentAccount\Model\CustomerBankAccount\DataProvider" name="customerbankaccount_form_data_source">
			<settings>
				<requestFieldName>customerba_id</requestFieldName>
				<primaryFieldName>customerba_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field formElement="input" name="customer_id" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Customer ID</label>
				<dataScope>customer_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="paymentaccount_bank_id" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Bank ID</label>
				<dataScope>paymentaccount_bank_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="account_owner" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Account Owner</label>
				<dataScope>account_owner</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="bank_branch" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Bank Branch</label>
				<dataScope>bank_branch</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="account_number" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Account Number</label>
				<dataScope>account_number</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="order" sortOrder="130">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Order</label>
				<dataScope>order</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="note" sortOrder="140">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Note</label>
				<dataScope>note</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="status" sortOrder="150">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">CustomerBankAccount</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
